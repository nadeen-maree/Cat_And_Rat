<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.learntodroid.androidminesweeper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/catandrat"
        android:testOnly="false"
        android:label="@string/app_name"
        android:roundIcon="@drawable/catandrat"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.CATvsRAT.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.CATvsRAT.GameActivity" />

    </application>

</manifest>